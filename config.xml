<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.phonegap.root" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>Worldofsteel</name>
  <description>
       Worldofsteel application.
    </description>
  <author email="support@phonegap.com" href="http://phonegap.com">
        Makerits Software Solutions
    </author>
  <content src="index.html"/>
